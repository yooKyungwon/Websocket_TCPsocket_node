	<script>
	var socket;


$(document).ready(function(){
//	socket = io.connect('175.126.195.162:4001');

	socket = io.connect();
socket.on('message',function(msg){
		
			var dataArray = msg.split('-');
			$('#Temperture').html(dataArray[0]);
			$('#Moisture').html(dataArray[1]);
			$('#Sunlight').html(dataArray[2]);



		// socket.emit('my other event',{my:'data'});

	});


	$('#btnUp').click(function(){
		
			window.alert("fasd");
			socket.emit("message","1");
			});
$('#btnDown').click(function(){
				socket.emit("message","2");
			});
$('#btnOn').click(function(){
				socket.emit("message","3");
			});

$('#btnOff').click(function(){
				socket.emit("message","4");
			});

	});



function send(arg){
		socket.emit(arg);
	}
	</script> 
	
